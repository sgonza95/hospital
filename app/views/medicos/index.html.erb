<p id="notice"><%= notice %></p>

<h1>Ficha Medicos</h1>

<table class=" table table-bordered table-hover">
  <thead>
    <tr>
      <th>Nombremedico</th>
      <th>Apemedico</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @medicos.each do |medico| %>
      <tr>
        <td><%= medico.nombremedico %></td>
        <td><%= medico.apemedico %></td>
        <td><%= link_to 'Show', medico %></td>
        <td><%= link_to 'Edit', edit_medico_path(medico) %></td>
        <td><%= link_to 'Destroy', medico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
